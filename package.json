{
  "name": "start-api",
  "version": "1.0.0",
  "description": "Secure Restful API with Nodejs, and Mongoose",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.js --exec babel-node --presets env",
    "test": "nyc --reporter=text --reporter=lcov --reporter=text-lcov mocha --delay --recursive test --require babel-polyfill --require babel-register --require ./test/index.js --exit",
    "test-local": "cross-env NODE_ENV=testlocal nyc --reporter=text --reporter=lcov --reporter=text-lcov mocha --delay --recursive test --require babel-polyfill --require babel-register --require ./test/index.js --exit",
    "sonar-local": "sonar-scanner -Dsonar.host.url=http://localhost:9002",
    "sonar": "node sonar-project.js",
    "build": "babel src -d dist --source-maps inline --copy-files",
    "heroku-postbuild": "npm install",
    "start": "node /usr/src/app/dist/app.js",
    "postinstall": "babel src -d dist --source-maps inline --copy-files"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.10.9",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "newrelic": "^7.0.1",
    "nodemailer": "^6.4.14",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "swagger-ui-express": "^3.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chai-subset": "^1.6.0",
    "cross-env": "^7.0.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.1.3",
    "nodemon": "^1.17.3",
    "nyc": "^15.1.0",
    "sonarqube-scanner": "^2.7.0"
  }
}
